<h1> Add a picture to the database</h1>

<%= link_to "back to picture index", pictures_url %>

<!-- form for binds a form to a model object. -->
<%= form_for @picture do |f| %>

<p>
	<%= f.label "artist" %><br>
	<%= f.text_field "artist" %>
</p>

<p>
	<%= f.label "title" %><br>
	<%= f.text_field "title" %>
</p>

<p>
	<%= f.label "url" %><br>
	<%= f.text_field "url" %>
</p>

<p>
	<%= f.submit "Save" %>
</p>

<% end %>